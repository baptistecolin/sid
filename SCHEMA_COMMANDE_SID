sid create name folder url 
#create a save "name" from "local_file/folder" to "url" 
sid list 
#list all the saves cashed on the computer 
sid setName name url
#set a name for a save at url
sid update name 
#update the save 
sid ls [version] name
#list saved content + version
sid delete name
#delete save
sid status name
#give the status of a save (last online version, url)
sid restore [version] [name]/[url] destination 
#download the save "name" in "version" (last version by default)
sid help
#get some help
sid about
#show the command options
