sid create name url local_file/folder 
#create a save "name" from "local_file/folder" to "url" 
sid list url 
#list all the saves at "url"
sid ls name url
#list all the folders/files of a save
sid update name url local_file/folder 
#update the save "name" at "url" 
sid dump name url
#dump save "name" at "url" 
sid status name url
#list the versions of "name" at "url" 
sid restore name url (version) 
#download the save "name" in "version" (last version by default)
sid help
#get some help
sid about
#show the command options
